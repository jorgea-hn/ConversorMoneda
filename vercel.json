{
    "version": 2,
    "builds": [
      { "use": "@vercel/python" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/app.py" }
    ],
    "env": {
      "FLASK_APP": "app.py"
    },
    "functions": { "api/*": { "runtime": "vercel-python@0.23.0" } },
    "web": "vercel.json",
    "command": "flask run --port $PORT"
  }
  
